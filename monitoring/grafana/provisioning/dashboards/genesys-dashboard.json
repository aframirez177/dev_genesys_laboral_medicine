{
  "dashboard": {
    "title": "Genesys Laboral Medicine - Overview",
    "tags": ["genesys", "api", "worker"],
    "timezone": "browser",
    "editable": true,
    "panels": [
      {
        "id": 1,
        "title": "HTTP Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(genesys_http_requests_total[5m])",
            "legendFormat": "{{method}} {{route}} ({{status_code}})"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "HTTP Request Duration (95th percentile)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(genesys_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{method}} {{route}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Active Requests",
        "type": "graph",
        "targets": [
          {
            "expr": "genesys_active_requests",
            "legendFormat": "Active Requests"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Active Jobs",
        "type": "graph",
        "targets": [
          {
            "expr": "genesys_active_jobs",
            "legendFormat": "{{job_type}}"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8}
      },
      {
        "id": 5,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "genesys_api_process_resident_memory_bytes / 1024 / 1024",
            "legendFormat": "RSS (MB)"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8}
      },
      {
        "id": 6,
        "title": "Job Processing Duration (95th percentile)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(genesys_job_duration_seconds_bucket[5m]))",
            "legendFormat": "{{job_type}} ({{status}})"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 7,
        "title": "Documents Generated",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(genesys_documents_generated_total[5m])",
            "legendFormat": "{{document_type}} ({{status}})"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 8,
        "title": "Cache Hit Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(genesys_cache_operations_total{result=\"hit\"}[5m]) / rate(genesys_cache_operations_total[5m]) * 100",
            "legendFormat": "Cache Hit Rate (%)"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24}
      },
      {
        "id": 9,
        "title": "Database Query Duration (95th percentile)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(genesys_db_query_duration_seconds_bucket[5m]))",
            "legendFormat": "{{query_type}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24}
      }
    ],
    "refresh": "30s",
    "schemaVersion": 16,
    "version": 1
  }
}
